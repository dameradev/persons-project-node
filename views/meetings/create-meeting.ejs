<body class="container-fluid">
  <%- include('../includes/navigation.ejs')%>
  <div class='container'>
  <h1>Create a meeting</h1>
  <form action="/meetings/create-meeting" method="POST">
    <label for="start">Starting time</label>
    <input type="time" name="start" id="start"><br>

    <label for="duration">Duration</label>
    <input type="time" name="duration" id="duration"><br>

    <label for="description">Description</label>
    <input type="textarea" name="description" id="description"><br>
    
    
    <button type="submit">Create Meeting</button>
  </form>
</div>
<%- include('../includes/end.ejs')%></br>